<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.bytecode.use_reflection_optimizer">false</property>
		<property name="hibernate.connection.password">$_gns_var_db_password</property>
		<property name="hibernate.connection.url">$_gns_var_db_url</property>
		<property name="hibernate.connection.username">$_gns_var_db_username</property>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQLInnoDBDialect</property>
		<property name="show_sql">true</property>
		<property name="format_sql">true</property>
		<property name="use_sql_comments">false</property>
		<property name="hibernate.hbm2ddl.auto">update</property>
		<!-- <mapping resource="com/mkyong/customer/hibernate/Customer.hbm.xml" /> -->
		<!-- <mapping class="com.mkyong.customer.model.Customer" /> -->
		<mapping class="com.cjs.basicweb.model.accesspath.AccessPath" />
		<mapping class="com.cjs.basicweb.model.module.Module" />
		<mapping class="com.cjs.basicweb.model.user.SimpleUser" />
		<mapping class="com.cjs.basicweb.model.usergroup.UserGroup" />
		<mapping class="com.cjs.basicweb.model.activitylog.ActivityLog" />
	</session-factory>
</hibernate-configuration>